# Example environment variables for gphotos-sync
# Copy this file to .env and customize as needed

# Deployment mode
# GPHOTOS_DEPLOYMENT_MODE=local

# Database configuration (for cloud mode)
# GPHOTOS_DATABASE_TYPE=postgresql
# GPHOTOS_DATABASE_POSTGRESQL_HOST=localhost
# GPHOTOS_DATABASE_POSTGRESQL_PORT=5432
# GPHOTOS_DATABASE_POSTGRESQL_DATABASE=gphotos
# GPHOTOS_DATABASE_POSTGRESQL_USERNAME=gphotos
# GPHOTOS_DATABASE_POSTGRESQL_PASSWORD=secret

# Storage configuration (for cloud mode)
# GPHOTOS_STORAGE_PROVIDER=s3
# GPHOTOS_STORAGE_S3_BUCKET=my-photos-bucket
# GPHOTOS_STORAGE_S3_REGION=us-east-1
# GPHOTOS_STORAGE_S3_ACCESS_KEY_ID=AKIAIOSFODNN7EXAMPLE
# GPHOTOS_STORAGE_S3_SECRET_ACCESS_KEY=wJalrXUtnFEMI/K7MDENG/bPxRfiCYEXAMPLEKEY

# Queue configuration (for cloud mode)
# GPHOTOS_QUEUE_BACKEND=redis
# GPHOTOS_QUEUE_REDIS_HOST=localhost
# GPHOTOS_QUEUE_REDIS_PORT=6379
# GPHOTOS_QUEUE_REDIS_PASSWORD=

# API configuration
# GPHOTOS_API_HOST=0.0.0.0
# GPHOTOS_API_PORT=8080

# Logging
# GPHOTOS_LOGGING_LEVEL=DEBUG
# GPHOTOS_LOGGING_FORMAT=json

# Resource limits
# GPHOTOS_RESOURCES_MAX_CPU_PERCENT=80.0
# GPHOTOS_RESOURCES_MAX_MEMORY_PERCENT=60.0
